<header class="header">
  <div class="wrapper header__flex">
    <figure class="header__logo"><img class="img-responsive" src="/assets/logo.jpeg" alt=""/></figure>
    <figure class="header__perfil"><img src="/assets/perfil.jpg" alt=""/></figure>

    <% if user_signed_in? %>
    <p>Bienvenido Acetatero <%= current_user.name %>
    </br>
    Tu password está siendo protegido: </br>
    <%= current_user.encrypted_password %>
      <li class="button">
        <%= link_to('Cerrar Sesión', destroy_user_session_path, :method => :delete) %> 
      </li>
      <li class="button">
        <%= link_to('Mi Perfil', profile_profile_path) %>    
      </li>
      <li class="button">
        <%= link_to('Editar Perfil', edit_user_registration_path) %>
      </li>
      </p>
    <% else %>
      <li class="button">
      <%= link_to('Iniciar Sesión', new_user_session_path)  %>
      </li>
      <li class="button">
      <%= link_to('Registrarme', new_user_registration_path)  %>
      </li>
    <% end %>

    <div class="header__nav icon-menu"></div>
  </div>
</header>
    
    <nav class="nav">
      <ul>
        <li><a href="<%= root_path %>">Inicio</a></li>
        <li><a href="<%= profile_profile_path %>">Mi Perfil</a></li>
        <li><a href="<%= new_new_path %>">Cultura Acetato</a></li>
        <li><a href="<%= artistas_prueba_path %>">Artistas</a></li>
      </ul>
    </nav>

